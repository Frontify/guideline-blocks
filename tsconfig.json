{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "module": "ES2022",
        "moduleResolution": "bundler",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "strict": true,
        "jsx": "react-jsx",
        "resolveJsonModule": true,
        "baseUrl": ".",
        "types": ["cypress", "cypress-real-events", "@4tw/cypress-drag-drop"]
    },
    "exclude": ["**/dist", "**/node_modules"]
}
