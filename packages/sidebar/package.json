{
    "name": "@frontify/guideline-block-sidebar",
    "version": "0.0.1",
    "description": "The sidebar containing the settings for the custom blocks",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "npm run clean && npm run build:components && npm run build:types",
        "build:components": "vite build",
        "build:storybook": "build-storybook -s .storybook/assets",
        "build:types": "tsc",
        "clean": "rimraf dist",
        "generate": "run-p generate:*",
        "generate:component-index-ts": "esno ./scripts/generateExports.ts",
        "prepare": "is-ci || npm run build",
        "prepublishOnly": "npm run build",
        "storybook": "start-storybook -p 6006 -s .storybook/assets",
        "typecheck": "tsc"
    },
    "author": "Frontify Developers <developers@frontify.com>",
    "repository": {
        "url": "https://github.com/Frontify/guideline-blocks.git"
    },
    "bugs": {
        "url": "https://github.com/Frontify/guideline-blocks/issues"
    },
    "devDependencies": {
        "@storybook/addon-actions": "6.3.12",
        "@storybook/addon-essentials": "6.3.12",
        "@storybook/addon-links": "6.3.12",
        "@storybook/addon-postcss": "2.0.0",
        "@storybook/react": "6.3.12",
        "@types/lodash-es": "4.17.5",
        "@types/node": "16.11.7",
        "@types/react": "17.0.34",
        "@types/react-dom": "17.0.11",
        "autoprefixer": "10.4.0",
        "esno": "0.12.0",
        "fast-glob": "3.2.7",
        "is-ci": "3.0.1",
        "postcss": "^8.3.11",
        "regenerator-runtime": "^0.13.9",
        "rimraf": "3.0.2",
        "storybook-builder-vite": "0.1.6",
        "storybook-dark-mode": "1.0.8",
        "tailwindcss": "2.2.19",
        "ts-node": "10.4.0",
        "typescript": "4.4.4",
        "vite": "2.6.14"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "@frontify/app-bridge": "^2.6.0",
        "@frontify/arcade": "^0.12.0",
        "lodash-es": "^4.17.21"
    }
}
