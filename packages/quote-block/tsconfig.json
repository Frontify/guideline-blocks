{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "strict": true,
        "target": "es6",
        "jsx": "react-jsx",
        "lib": ["ES2021"],
        "resolveJsonModule": true,
        "types": ["cypress"],
        "skipLibCheck": true
    },
    "exclude": ["**/dist", "**/node_modules"]
}
