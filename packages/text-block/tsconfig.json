{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "strict": true,
        "target": "es6",
        "jsx": "react-jsx",
        "lib": ["ES2021", "DOM"]
    },
    "exclude": ["**/dist", "**/node_modules"]
}
