{
    "name": "@frontify/guideline-blocks-shared",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "sideEffects": false,
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && tsc && npm run build:types",
        "build:watch": "npm run clean && tsc -w --declaration",
        "build:types": "tsc --declaration --emitDeclarationOnly",
        "test": "jest",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "prettier": "prettier --check src",
        "prettier:fix": "prettier --write src",
        "typecheck": "tsc --noEmit"
    },
    "private": true,
    "author": "Frontify Developers <developers@frontify.com>",
    "devDependencies": {
        "@cypress/react": "5.10.3",
        "@frontify/eslint-config-typescript": "0.13.2",
        "@frontify/guideline-blocks-settings": "*",
        "@types/jest": "27.0.3",
        "@types/react": "17.0.37",
        "@types/sinon": "10.0.6",
        "@types/tinycolor2": "^1.4.3",
        "cypress": "9.1.1",
        "eslint": "8.4.1",
        "jest": "27.4.5",
        "prettier": "2.5.1",
        "rimraf": "^3.0.2",
        "ts-jest": "27.1.2",
        "ts-node": "10.4.0",
        "typescript": "4.5.4"
    },
    "dependencies": {
        "@frontify/app-bridge": "2.11.0",
        "@frontify/arcade": "1.0.0",
        "sinon": "13.0.0",
        "tinycolor2": "^1.4.2"
    }
}
