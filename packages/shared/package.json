{
    "name": "@frontify/guideline-blocks-shared",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "sideEffects": false,
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js"
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && tsc && npm run build:types",
        "build:types": "tsc --declaration --emitDeclarationOnly",
        "test": "jest",
        "lint": "eslint src",
        "lint:fix": "eslint --fix src",
        "prettier": "prettier --check src",
        "prettier:fix": "prettier --write src",
        "typecheck": "tsc --noEmit"
    },
    "private": true,
    "author": "Frontify Developers <developers@frontify.com>",
    "devDependencies": {
        "@cypress/react": "5.10.3",
        "@frontify/eslint-config-typescript": "0.12.0",
        "@frontify/guideline-blocks-settings": "^0.0.1-alpha.0",
        "@types/jest": "27.0.3",
        "@types/react": "17.0.37",
        "@types/sinon": "10.0.6",
        "cypress": "9.1.1",
        "eslint": "8.3.0",
        "jest": "27.4.3",
        "prettier": "2.5.1",
        "rimraf": "^3.0.2",
        "ts-jest": "27.1.1",
        "ts-node": "10.4.0",
        "typescript": "4.5.2"
    },
    "dependencies": {
        "@frontify/app-bridge": "^2.10.0",
        "sinon": "12.0.1"
    }
}
